{
  "name": "lokielse/laravel-sls",
  "type": "library",
  "description": "Aliyun SLS Log For Laravel",
  "keywords": [
    "laravel",
    "lokielse",
    "aliyun",
    "sls",
    "log",
    "driver",
    "cloud",
    "日志",
    "阿里云",
    "laravel-sls"
  ],
  "homepage": "https://github.com/lokielse/laravel-sls",
  "license": "MIT",
  "authors": [
    {
      "name": "lokielse",
      "email": "lokielse@gmail.com",
      "homepage": "http://lokielse.org",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.1",
    "php": "~5.5|~7.0",
    "lokielse/aliyun-open-api-sls": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Lokielse\\LaravelSLS\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lokielse\\LaravelSLS\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
